{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/components/*": ["src/components/*"],
      "~/screens/*": ["src/screens/*"],
      "~/navigations/*": ["src/navigations/*"],
      "~/types/*": ["src/types/*"],
      "~/mock/*": ["src/mock/*"]
    },
    "typeRoots": ["./types"]
  },
}
